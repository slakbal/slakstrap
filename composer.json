{
  "name": "slakbal/slakstrap",
  "description": "Twitter Bootstrap 4-beta markup generator for laravel",
  "license": "MIT",
  "keywords": [
    "bootstrap",
    "laravel"
  ],
  "authors": [
    {
      "name": "Leslie Price",
      "email": "leslie.price78@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "laravelcollective/html": "^5",
    "illuminate/support": "~5"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Slakbal\\Slakstrap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slakbal\\Slakstrap\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Slakbal\\Slakstrap\\SlakstrapServiceProvider"
      ],
      "aliases": {
        "Form": "Collective\\Html\\FormFacade",
        "Html": "Collective\\Html\\HtmlFacade"
      }
    }
  },
  "minimum-stability": "stable"
}
